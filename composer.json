{
    "name": "comes/nova-scheduler-ui",
    "description": "This package is a simple nova based UI for DionTech/laravel-extended-scheduler. Manage your scheduler the graphical way.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "diontech/laravel-extended-scheduler" : "1.*"
    },
    "autoload": {
        "psr-4": {
            "Comes\\NovaSchedulerUi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Comes\\NovaSchedulerUi\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
